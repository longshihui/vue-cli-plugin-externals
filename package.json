{
    "name": "vue-cli-plugin-externals",
    "version": "2.0.2",
    "description": "Manage external modules in your project in vue-cli 3.x",
    "main": "./service/index.js",
    "scripts": {
        "test": "jest",
        "release": "standard-version"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:longshihui/vue-cli-plugin-externals.git"
    },
    "homepage": "https://github.com/longshihui/vue-cli-plugin-externals#readme",
    "bugs": {
        "url": "https://github.com/longshihui/vue-cli-plugin-externals/issues"
    },
    "keywords": [
        "vue",
        "vue-cli",
        "vue-cli-plugin",
        "externals",
        "cdn-modules"
    ],
    "author": "colorless",
    "license": "ISC",
    "devDependencies": {
        "@vue/cli": "^3.4.1",
        "@vue/cli-service": "^3.4.1",
        "commitizen": "^3.0.4",
        "cz-conventional-changelog": "^2.1.0",
        "husky": "^1.1.2",
        "jest": "^24.1.0",
        "lint-staged": "^8.0.3",
        "prettier": "^1.14.3",
        "rimraf": "^2.6.3",
        "standard-version": "^8.0.1",
        "vue-template-compiler": "^2.6.8"
    },
    "dependencies": {
        "html-webpack-externals-plugin": "^3.8.0",
        "lodash": "^4.17.11"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "**/*.{js,vue,json,css,md}": [
            "prettier --write",
            "git add"
        ]
    }
}
